{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "write-poetry/wp-test",
	"version": "0.1.0",
	"title": "Animation",
	"category": "design",
	"icon": "smiley",
	"description": "Animate inner content.",
	"attributes": {
		"animationName": {
			"type": "string",
			
			"enum": [ "backInDown", "backInLeft", "backInRight", "backInUp" ]
		},
		"animationSpeed": {
			"type": "string",
			"enum": [ "slow", "slower", "fast", "faster" ]
		},
		"animationDelay": {
			"type": "string",
			"enum": [ "2", "3", "4", "5" ]
		},
		"animationRepeat": {
			"type": "string",
			"enum": [ "repeat-1", "repeat-2", "repeat-3", "infinite" ]
		}
	},
	"example": {
        "attributes": {
            "animationName": "bounce",
            "animationSpeed": "fast",
            "animationDelay": "2",
            "animationRepeat": "infinite"
        }
    },
	"supports": {
		"html": false
	},
	"textdomain": "wp-test",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
